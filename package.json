{
    "name": "markdownlint-config-fluid",
    "version": "0.1.4",
    "description": "Shareable MarkdownLint config for the fluid project.",
    "main": "index.js",
    "scripts": {
        "lint": "fluid-lint-all",
        "release": "standard-version -t ''",
        "test": "ava"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fluid-project/markdownlint-config-fluid.git"
    },
    "keywords": [
        "markdownlint",
        "fluid",
        "fluid-project"
    ],
    "author": "OCAD University <idrc@ocadu.ca>",
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/fluid-project/markdownlint-config-fluid/issues"
    },
    "homepage": "https://github.com/fluid-project/markdownlint-config-fluid#readme",
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "dependencies": {
        "infusion": "3.0.0-dev.20210312T233557Z.0b016a6dc.FLUID-6580",
        "markdownlint": "0.29.0"
    },
    "devDependencies": {
        "@commitlint/cli": "17.6.6",
        "@commitlint/config-conventional": "11.0.0",
        "ava": "3.13.0",
        "eslint-config-fluid": "^2.0.0",
        "fluid-lint-all": "1.1.1",
        "husky": "4.3.8",
        "standard-version": "9.0.0"
    }
}

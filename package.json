{
    "name": "markdownlint-config-fluid",
    "version": "0.1.4",
    "description": "Shareable MarkdownLint config for the fluid project.",
    "main": "index.js",
    "scripts": {
        "lint": "fluid-lint-all",
        "release": "standard-version -t ''",
        "test": "ava"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fluid-project/markdownlint-config-fluid.git"
    },
    "keywords": [
        "markdownlint",
        "fluid",
        "fluid-project"
    ],
    "author": "OCAD University <idrc@ocadu.ca>",
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/fluid-project/markdownlint-config-fluid/issues"
    },
    "homepage": "https://github.com/fluid-project/markdownlint-config-fluid#readme",
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "dependencies": {
        "infusion": "4.0.0-dev.20211216T180403Z.d5f6173fc.FLUID-6703",
        "markdownlint": "0.25.1"
    },
    "devDependencies": {
        "@commitlint/cli": "16.2.1",
        "@commitlint/config-conventional": "16.2.1",
        "ava": "4.0.1",
        "eslint-config-fluid": "^2.0.0",
        "fluid-lint-all": "1.1.5",
        "husky": "4.3.8",
        "standard-version": "9.3.2"
    }
}
